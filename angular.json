{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hotmaps-toolbox-client": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/assets/fonts/flaticon.css",
              "node_modules/leaflet-draw/dist/leaflet.draw.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/uikit/dist/css/uikit.css",
              "src/assets/fonts/Roboto.css",
              "node_modules/leaflet-control-geocoder/dist/Control.Geocoder.css",
              "node_modules/leaflet-dvf/dist/css/dvf.css",
              "src/styles.css"
            ],
            "scripts": [
              "node_modules/stacktrace-js/dist/stacktrace.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet-virtual-grid/dist/virtual-grid.js",
              "node_modules/leaflet-draw/dist/leaflet.draw.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/uikit/dist/js/uikit.min.js",
              "node_modules/uikit/dist/js/uikit-icons.min.js",
              "node_modules/leaflet-control-geocoder/dist/Control.Geocoder.js",
              "node_modules/leaflet-dvf/dist/leaflet-dvf.min.js"
            ]
          },
          "configurations": {
            "prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hotmaps-toolbox-client:build"
          },
          "configurations": {
            "prod": {
              "browserTarget": "hotmaps-toolbox-client:build:prod"
            },
            "production": {
              "browserTarget": "hotmaps-toolbox-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hotmaps-toolbox-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/stacktrace-js/dist/stacktrace.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/leaflet/dist/leaflet.js",
              "node_modules/leaflet-virtual-grid/dist/virtual-grid.js",
              "node_modules/leaflet-draw/dist/leaflet.draw.js",
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/uikit/dist/js/uikit.min.js",
              "node_modules/uikit/dist/js/uikit-icons.min.js",
              "node_modules/leaflet-control-geocoder/dist/Control.Geocoder.js",
              "node_modules/leaflet-dvf/dist/leaflet-dvf.min.js"
            ],
            "styles": [
              "src/assets/fonts/flaticon.css",
              "node_modules/leaflet-draw/dist/leaflet.draw.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/uikit/dist/css/uikit.css",
              "src/assets/fonts/Roboto.css",
              "node_modules/leaflet-control-geocoder/dist/Control.Geocoder.css",
              "node_modules/leaflet-dvf/dist/css/dvf.css",
              "src/styles.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "hotmaps-toolbox-client-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "hotmaps-toolbox-client:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "hotmaps-toolbox-client",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}